{
  "name": "puppet-nfs",
  "version": "0.0.1",
  "author": "James Eckersall",
  "summary": "Installs, configures, and manages NFS",
  "license": "Apache-2.0",
  "source": "https://github.com/jameseck/puppet-nfs",
  "project_page": "https://github.com/jameseck/puppet-nfs",
  "issues_url": "https://github.com/jameseck/puppet-nfs/issues",
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 3.2.0"},
    {"name":"jameseck/etcservices","version_requirement":">= 0.0.1"}
  ],
  "data_provider": "hiera",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.5.0 < 6.0.0"
    }
  ]
}
